{
  "name": "remix-vite-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm run clean && node ./server.mjs",
    "build": "npm run clean && vite build && vite build --ssr",
    "start": "cross-env NODE_ENV=production node server.mjs",
    "typecheck": "tsc",
    "lint": "eslint .",
    "format": "prettier --write .",
    "clean": "rm -rf build public/build",
    "nuke": "npm run clean && rm -rf node_modules package-lock.json && npm i"
  },
  "dependencies": {
    "@remix-run/css-bundle": "2.3.1",
    "@remix-run/express": "^2.3.1",
    "@remix-run/node": "2.3.1",
    "@remix-run/react": "2.3.1",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.7.1",
    "react": "18.3.0-canary-640ccebb7-20231201",
    "react-dom": "18.3.0-canary-640ccebb7-20231201"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.11",
    "@remix-run/dev": "2.3.1",
    "@remix-run/eslint-config": "2.3.1",
    "@types/eslint": "^8.44.8",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "morgan": "^1.10.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.8",
    "remix-flat-routes": "^0.6.4",
    "tailwindcss": "^3.3.6",
    "typescript": "5.1.6",
    "vite": "^5.0.5",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ]
}
